ALL : BUILD\SND.EXP BUILD\ELEVOL.EXP

BUILD\SND.EXP : BUILD\SND.OBJ
	386LINK BUILD\SND.OBJ -EXE BUILD\SND.EXP @LIBS.LN

BUILD\SND.OBJ : SND.C
	HCD386 SND.C -OBJ BUILD\SND.OBJ


BUILD\ELEVOL.EXP : BUILD\ELEVOL.OBJ
	386LINK BUILD\ELEVOL.OBJ -EXE BUILD\ELEVOL.EXP @LIBS.LN

BUILD\ELEVOL.OBJ : ELEVOL.C
	HCD386 ELEVOL.C -OBJ BUILD\ELEVOL.OBJ
